<?xml version="1.0"?>
<phpunit
	bootstrap="/var/www/html/tests/bootstrap.php"
	backupGlobals="false"
	colors="true"
	convertErrorsToExceptions="true"
	convertNoticesToExceptions="true"
	convertWarningsToExceptions="true"
	>
	<testsuites>
		<testsuite name="fileSystem">
			<directory prefix="test-" suffix=".php">/var/www/html/tests/</directory>
		</testsuite>
	</testsuites>
</phpunit>
